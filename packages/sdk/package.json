{
  "name": "@oraichain/oraidex-evm-sdk",
  "description": "Oraidex EVM SDK",
  "repository": "https://github.com/oraidex/evm-entry-point.git",
  "keywords": [
    "oraichain",
    "ethereum",
    "oraidex"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "lint": "eslint . --config eslint.config.js",
    "lint:fix": "eslint . --config eslint.config.js --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@oraichain/oraidex-contracts-sdk": "^1.0.56",
    "@oraichain/osor-api-contracts-sdk": "^1.0.3",
    "axios": "^1.8.3",
    "decimal.js": "^10.5.0",
    "long": "^5.3.1",
    "tslib": "^2.6.2",
    "zod": "^3.24.2"
  },
  "devDependencies": {
    "@types/node": "^22.13.10",
    "@vitest/coverage-v8": "^3.0.8",
    "eslint": "^9.21.0",
    "eslint-config-prettier": "^10.1.1",
    "globals": "^13.24.0",
    "prettier": "^3.5.3",
    "typescript": "^5.8.2",
    "typescript-eslint": "^8.24.1",
    "vitest": "^3.0.8"
  }
